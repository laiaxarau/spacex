---
import { getPhotoIndex } from "../services/spacex";

const imageIndex = await getPhotoIndex();
---

<style>
  img {
    mask-image: linear-gradient(
      to top,
      transparent 10%,
      #000 15% 90%,
      transparent 95%
    );
    mask-size: 135%;
    mask-position: center;
    mask-repeat: no-repeat;
    mask-composite: intersect;
  }
</style>
<img src={imageIndex} alt="" class="w-full" />
